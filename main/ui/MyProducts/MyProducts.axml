<view class="container" style="padding-top: {{titleBarHeight + statusBarHeight + 25}}px">

  <view class="container-item ">
    <view class="descriptionAcc">
      <view class="container-header-display" style="text-align:left">
        <text style="font-weight:600">{{name}}</text>
        <text>{{Doc}}. {{nit}}</text>
      </view>
    </view>
    <view class="container-header-display" style="text-align:right">
      <text style="font-weight:600">Cuenta</text>
      <text>{{numberAccount}}</text>
    
    </view>
  </view>
  <view class="container-Bundle">
    <view class="bundleInternet">
      <view class="descriptionIcon">
        <view class="text-description">
          <view class="icons-display">
            <image id="icon-phone" mode="scaleToFill" src="/main/ui/assets/icons/icono-globe-ppal.svg" />
            <view class="text-static-Icon">
              Internet
            </view>
          </view>
          <view class="text-dinamic">
            {{DescripctionPlanInternet}}
          </view>
          <view class="text-static">
            Navegación
          </view>
        </view>
      </view>
      <view onTap="ActiveModal" class="btn-MejoraPlan">
        Mejora tu plan
      </view>
    </view>
  </view>

  <view class="container-Bundle">
    <view class="bundleInternet">
      <view class="descriptionIcon">
        <view class="text-description">
           <view class="icons-display">
            <image id="icon-phone" mode="scaleToFill" src="/main/ui/assets/icons/icono-phone-call-ppal.svg" />
            <view class="text-static-Icon">
            Telefonía
            </view>
           </view>
       
          <view class="text-dinamic">
            {{DescripctionPlanPhone}}
          </view>
          <view a:if="{{!isPhonePlan}}" class="text-static">
            Actualemente no cuentas con este servicio
          </view>
          <view a:if="{{isPhonePlan}}" class="text-static">
            Llamadas Internacionales
          </view>
        </view>
      </view>
      <view a:if="{{isPhonePlan}}">
        <arrow-button
          onTap="onNavigateactivatePhone"
          style="background-color:#FFFFFF; color: #808080; font-weight: bold;height:70rpx"
          stateImg={{true}}
          imgSrc="/main/ui/assets/icons/icono-phone-ppal.svg"
          iconArrowSrc="/main/ui/assets/icons/navigateNextBlack.svg"
          dataInfo="{{ data }}"
        >
          Activa tu teléfono virtual
        </arrow-button>
      </view>
      <view a:if="{{isPhonePlan}}">
        <arrow-button
          onTap="onNavigate"
          style="background-color:#FFFFFF;color: #808080; font-weight: bold;height:70rpx"
          stateImg={{true}}
          imgSrc="/main/ui/assets/icons/icono-download-ppal.svg"
          iconArrowSrc="/main/ui/assets/icons/navigateNextBlack.svg"
          dataInfo="{{ data }}"
        >
          Descarga teléfono virtual
        </arrow-button>
      </view>
      <view onTap="ActiveModal" class="btn-MejoraPlan">
        Mejora tu plan
      </view>
    </view>
  </view>

  <view class="container-Bundle">
    <view class="bundleInternet">
      <view class="descriptionIcon">
        <view class="text-description">
          <view class="icons-display">
          <image id="icon-phone" mode="scaleToFill" src="/main/ui/assets/icons/icono-tv-ppal.svg" />
          <view class="text-static-Icon">
            Televisión
          </view>
        </view>
          <view class="text-dinamic">
            Televisión
          </view>
          <view a:if="{{!isTvPlan}}" class="text-static">
            Actualemente no cuentas con este servicio
          </view>
          <view
            a:for="{{DescripctionPlantv}}"
            a:for-index="index"
            a:for-item="item"
            if="{{isTvPlan}}"
          >
            <view class="text-static">
              {{item}}
            </view>
          </view>

        </view>
      </view>
      <arrow-button
          onTap="redirectPaquetesContratados"
          style="background-color: #FFFFFF; color: #808080; font-weight: bold;height:70rpx"
          stateImg={{true}}
          imgSrc="/main/ui/assets/icons/icono-archive-ppal.svg"
          iconArrowSrc="/main/ui/assets/icons/navigateNextBlack.svg"
          dataInfo="{{ data }}"
        >
          Activa tu teléfono virtual
        </arrow-button>
    
      <view onTap="ActiveModal" class="btn-MejoraPlan">
        Mejora tu plan
      </view>
    </view>
  </view>

  <modal-accept-cancel
    content="Al ingresar a esta sección podria consumir datos de su plan. ¿Desea continuar?"
    visible="{{modalVisible}}"
    onClose="handleClose"
    primaryButtonText="Aceptar"
    secondaryButtonText="Cerrar"
    onPrimaryButtonTap="onAcceptButtonTap"
    onSecondaryButtonTap="onCancelButtonTap"
  />

  <modal-accept-cancel
    content="Al ingresar a esta sección podria consumir datos de su plan. ¿Desea continuar?"
    visible="{{modalVisiblePhoneActivate}}"
    onClose="handleClose"
    primaryButtonText="Aceptar"
    secondaryButtonText="Cerrar"
    onPrimaryButtonTap="onAcceptButtonTapPhoneActivate"
    onSecondaryButtonTap="onCancelButtonTap"
  />
  <modal-accept-cancel
    content="Al ingresar a esta sección podria consumir datos de su plan. ¿Desea continuar?"
    visible="{{modalVisiblePhonePlayStore}}"
    onClose="handleClose"
    primaryButtonText="Aceptar"
    secondaryButtonText="Cerrar"
    onPrimaryButtonTap="onAcceptButtonTapPhonePlayStore"
    onSecondaryButtonTap="onCancelButtonTap"
  />
  <modal-accept
    content="{{descriptionError}}"
    visible="{{modalVisibleError}}"
    onClose="{{redirectServices}}"
    primaryButtonText="Aceptar"
    onPrimaryButtonTap="{{redirectServices}}"
  />
  <loading-component a:if="{{showLoading}}" />
</view>